#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:43
#, python-format
msgid "$%(amount)s/month"
msgstr "$%(amount)s/mese"

#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:39
#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:47
msgid "A coffee"
msgstr "Un caffè"

#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:40
#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:48
msgid "A snack"
msgstr "Uno snack"

#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:41
#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:49
msgid "A pizza"
msgstr "Una pizza"

#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:42
#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:50
msgid "A movie night for one"
msgstr "Una serata al cinema per uno"

#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:43
#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:51
msgid "A frugal dinner out for one"
msgstr "Una cena economica per uno"

#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:46
msgid "Yearly contributions (cheaper!)"
msgstr "Contributi annuali (più economico!)"

#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:47
#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:48
#: templates/astrobin_apps_donations/inclusion_tags/_paypal_button.html:49
