	@list='$(dist_docdata_DATA)'; test -n "$(docdatadir)" || list=; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(docdatadir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(docdatadir)" || exit $$?; \
	done

uninstall-dist_docdataDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(dist_docdata_DATA)'; test -n "$(docdatadir)" || list=; \
	files=`for p in $$list; do echo $$p; done | sed -e 's|^.*/||'`; \
	test -n "$$files" || exit 0; \
