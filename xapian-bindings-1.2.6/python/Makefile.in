install-pkgpylibLTLIBRARIES: $(pkgpylib_LTLIBRARIES)
	@$(NORMAL_INSTALL)
	test -z "$(pkgpylibdir)" || $(MKDIR_P) "$(DESTDIR)$(pkgpylibdir)"
	@list='$(pkgpylib_LTLIBRARIES)'; test -n "$(pkgpylibdir)" || list=; \
	list2=; for p in $$list; do \
	  if test -f $$p; then \
	    list2="$$list2 $$p"; \
	  else :; fi; \
	done; \
	test -z "$$list2" || { \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 '$(DESTDIR)$(pkgpylibdir)'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=install $(INSTALL) $(INSTALL_STRIP_FLAG) $$list2 "$(DESTDIR)$(pkgpylibdir)"; \
	}

uninstall-pkgpylibLTLIBRARIES:
	@$(NORMAL_UNINSTALL)
	@list='$(pkgpylib_LTLIBRARIES)'; test -n "$(pkgpylibdir)" || list=; \
	for p in $$list; do \
	  $(am__strip_dir) \
	  echo " $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f '$(DESTDIR)$(pkgpylibdir)/$$f'"; \
	  $(LIBTOOL) $(AM_LIBTOOLFLAGS) $(LIBTOOLFLAGS) --mode=uninstall rm -f "$(DESTDIR)$(pkgpylibdir)/$$f"; \
	done

clean-pkgpylibLTLIBRARIES:
	-test -z "$(pkgpylib_LTLIBRARIES)" || rm -f $(pkgpylib_LTLIBRARIES)
	@list='$(pkgpylib_LTLIBRARIES)'; for p in $$list; do \
	  dir="`echo $$p | sed -e 's|/[^/]*$$||'`"; \
	  test "$$dir" != "$$p" || dir=.; \
	  echo "rm -f \"$${dir}/so_locations\""; \
	  rm -f "$${dir}/so_locations"; \
	done
