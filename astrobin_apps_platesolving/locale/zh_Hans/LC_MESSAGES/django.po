msgstr "主作品页面上标注的字体大小"

msgid "Show equatorial grid"
msgstr "显示赤道网格"

msgid "Show ecliptic"
msgstr "显示黄道"

msgid "Show galactic equator"
msgstr "显示银河赤道"

msgid "Show constellation borders"
msgstr "显示星座边界"

msgid "Show constellation lines"
msgstr "显示星座连线"

